<?xml version="1.0" encoding="UTF-8"?>
<svg width="400" height="400" viewBox="0 0 400 400" xmlns="http://www.w3.org/2000/svg">
  <!-- Logo baseada na referência enviada -->
  <defs>
    <style>
      .logo-text { font-family: 'Impact', 'Arial Black', sans-serif; font-weight: 900; }
      .logo-subtitle { font-family: 'Arial', sans-serif; font-weight: 600; }
    </style>
  </defs>
  
  <!-- Círculo externo -->
  <circle cx="200" cy="200" r="190" fill="none" stroke="white" stroke-width="8"/>
  
  <!-- Texto superior curvado "SONS OF PEAKY" -->
  <path id="top-curve" d="M 50,200 A 150,150 0 0,1 350,200" fill="none" stroke="none"/>
  <text class="logo-text" fill="white" font-size="36" letter-spacing="3">
    <textPath href="#top-curve" startOffset="50%" text-anchor="middle">
      SONS OF PEAKY
    </textPath>
  </text>
  
  <!-- Texto "ORIGINAL" no topo -->
  <text class="logo-subtitle" x="200" y="40" fill="white" font-size="18" text-anchor="middle" letter-spacing="2">ORIGINAL</text>
  
  <!-- Capacete e caveira central -->
  <g transform="translate(200,200)">
    <!-- Capacete -->
    <path d="M -60,-80 Q -80,-60 -80,-20 Q -80,20 -60,40 L 60,40 Q 80,20 80,-20 Q 80,-60 60,-80 Z" fill="white" stroke="black" stroke-width="2"/>
    <!-- Visor/óculos -->
    <ellipse cx="-25" cy="-40" rx="20" ry="15" fill="black"/>
    <ellipse cx="25" cy="-40" rx="20" ry="15" fill="black"/>
    <!-- Ponte dos óculos -->
    <rect x="-5" y="-42" width="10" height="4" fill="black"/>
    
    <!-- Caveira -->
    <ellipse cx="0" cy="-10" rx="50" ry="40" fill="white" stroke="black" stroke-width="2"/>
    <!-- Olhos da caveira -->
    <ellipse cx="-20" cy="-15" rx="8" ry="12" fill="black"/>
    <ellipse cx="20" cy="-15" rx="8" ry="12" fill="black"/>
    <!-- Nariz -->
    <path d="M 0,-5 L -5,10 L 5,10 Z" fill="black"/>
    
    <!-- Barba -->
    <g transform="translate(0,15)">
      <!-- Barba base -->
      <path d="M -40,10 Q -30,25 -20,35 Q -10,45 0,50 Q 10,45 20,35 Q 30,25 40,10 L 40,20 Q 30,35 20,45 Q 10,55 0,60 Q -10,55 -20,45 Q -30,35 -40,20 Z" fill="white" stroke="black" stroke-width="2"/>
      <!-- Linhas da barba -->
      <path d="M -30,15 Q -20,25 -10,30" fill="none" stroke="black" stroke-width="1"/>
      <path d="M -25,20 Q -15,30 -5,35" fill="none" stroke="black" stroke-width="1"/>
      <path d="M -20,25 Q -10,35 0,40" fill="none" stroke="black" stroke-width="1"/>
      <path d="M 30,15 Q 20,25 10,30" fill="none" stroke="black" stroke-width="1"/>
      <path d="M 25,20 Q 15,30 5,35" fill="none" stroke="black" stroke-width="1"/>
      <path d="M 20,25 Q 10,35 0,40" fill="none" stroke="black" stroke-width="1"/>
    </g>
  </g>
  
  <!-- Texto inferior esquerdo -->
  <text class="logo-subtitle" x="80" y="320" fill="white" font-size="14" text-anchor="middle" letter-spacing="1">BORN FROM</text>
  <text class="logo-subtitle" x="80" y="340" fill="white" font-size="12" text-anchor="middle" letter-spacing="1">HANDCRAFTED</text>
  
  <!-- Texto inferior direito -->
  <text class="logo-subtitle" x="320" y="320" fill="white" font-size="14" text-anchor="middle" letter-spacing="1">THE ROAD</text>
  <text class="logo-subtitle" x="320" y="340" fill="white" font-size="12" text-anchor="middle" letter-spacing="1">BRAZILIAN MADE</text>
  
  <!-- Linha horizontal -->
  <line x1="50" y1="280" x2="350" y2="280" stroke="white" stroke-width="2"/>
</svg>